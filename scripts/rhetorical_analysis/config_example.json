{
  "_comment": "Configuration d'exemple pour Unified Production Analyzer",
  "_version": "1.0.0",
  "_description": "Param√®tres de configuration pour l'analyse en production",
  
  "llm_service": "openai",
  "llm_model": "gpt-4",
  "llm_temperature": 0.3,
  "llm_max_tokens": 2000,
  "llm_timeout": 60,
  "llm_retry_count": 3,
  "llm_retry_delay": 1.0,
  
  "logic_type": "fol",
  "enable_fallback": true,
  "fallback_order": ["fol", "propositional"],
  
  "mock_level": "none",
  "require_real_gpt": true,
  "require_real_tweety": true,
  "require_full_taxonomy": true,
  "validate_tools": true,
  
  "orchestration_type": "unified",
  "enable_conversation_trace": true,
  "enable_micro_orchestration": false,
  "max_agents": 5,
  
  "analysis_modes": ["unified"],
  "batch_size": 10,
  "enable_parallel": true,
  "max_workers": 4,
  
  "tweety_retry_count": 5,
  "tweety_retry_delay": 2.0,
  "tweety_timeout": 30,
  "tweety_memory_limit": "2g",
  
  "output_format": "json",
  "enable_detailed_logs": true,
  "save_intermediate": false,
  "report_level": "production",
  
  "validate_inputs": true,
  "validate_outputs": true,
  "check_dependencies": true,
  
  "_presets": {
    "production": {
      "mock_level": "none",
      "require_real_gpt": true,
      "require_real_tweety": true,
      "check_dependencies": true,
      "report_level": "production"
    },
    "development": {
      "mock_level": "partial",
      "require_real_gpt": true,
      "require_real_tweety": false,
      "check_dependencies": false,
      "report_level": "debug"
    },
    "testing": {
      "mock_level": "full",
      "require_real_gpt": false,
      "require_real_tweety": false,
      "check_dependencies": false,
      "report_level": "minimal"
    }
  }
}