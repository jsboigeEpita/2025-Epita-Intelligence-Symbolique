{
  "service_type": "openai",
  "api_key_env_var": "OPENAI_API_KEY",
  "model": "gpt-4",
  "temperature": 0.2,
  "max_tokens": 2000,
  "top_p": 1.0,
  "frequency_penalty": 0.0,
  "presence_penalty": 0.0,
  "timeout": 60,
  "retry_count": 3,
  "retry_delay": 5,
  "system_message": "Vous êtes un assistant spécialisé dans l'analyse d'argumentation. Votre tâche est d'analyser des textes pour en extraire les arguments, les prémisses et les conclusions.",
  "fallback_models": [
    {
      "model": "gpt-3.5-turbo",
      "temperature": 0.3,
      "max_tokens": 1500
    },
    {
      "model": "claude-2",
      "temperature": 0.2,
      "max_tokens": 2000
    }
  ],
  "logging": {
    "enabled": true,
    "log_level": "INFO",
    "log_requests": true,
    "log_responses": true
  },
  "caching": {
    "enabled": true,
    "ttl": 86400,
    "max_cache_size": 1000
  }
}